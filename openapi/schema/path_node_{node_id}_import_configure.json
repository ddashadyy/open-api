{
  "paths": {
    "/node/{node_id}/import/configure": {
      "post": {
        "tags": [
          "node"
        ],
        "summary": "Configuring imported node",
        "description": "Configuring imported node",
        "operationId": "configure_imported_node",
        "parameters": [
          {
            "name": "node_id",
            "in": "path",
            "description": "Node ID",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "requestBody": {
          "description": "Params",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "network_autosetup_disabled": {
                    "type": "boolean",
                    "default": false
                  }
                },
                "additionalProperties": false
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Node configure started successfully",
            "content": {
              "application/json": {
               "schema": {
                  "$ref": "#/components/schemas/IdTask"
                }
              }
            }
          }
        }
      }
    }
  }
}

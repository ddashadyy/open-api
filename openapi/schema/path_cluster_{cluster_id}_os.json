{
  "paths": {
    "/cluster/{cluster_id}/os": {
      "get": {
        "tags": [
          "cluster"
        ],
        "summary": "Cluster os list",
        "operationId": "get_cluster_os_list",
        "parameters": [
          {
            "name": "cluster_id",
            "in": "path",
            "description": "Cluster ID",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Cluster os list is successfully received",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "last_notify": {
                      "$ref": "#/components/schemas/LastNotify"
                    },
                    "list": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "integer"
                          },
                          "name": {
                            "type": "string"
                          },
                          "os_group": {
                            "type": "string"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
